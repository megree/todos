{
  "manifest_version": 2,

  "name": "TODOs",
  "description": "Simple to do list in minimalist visual aesthetics",
  "version": "0.0.2",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "TODOs"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "web_accessible_resources": ["styles/contentscript.css", "SourceSansPro.ttf"],
  "icons": { "16": "icon.png","48": "icon.png","128": "icon.png"}
}